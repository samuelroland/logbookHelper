{
  "manifest_version": 2,
  "name": "logbookHelper",
  "version": "0.1",
  "author": "Samuel Roland",
  "homepage_url": "https://github.com/samuelroland/logbookHelper",
  "description": "Internship logbook helper for CPNV students",
  "permissions": ["https://intranet.cpnv.ch/stages/Journal.php"],
  "icons": {
    "19": "icons/19.png",
    "48": "icons/48.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "logbookHelper",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
